{
	"name" : "t3ac/t3up-container",
	"description" : "T3UP for EXT:container",
	"authors" : [{
			"name" : "Michael Lang",
			"role" : "Developer",
			"email" : "michael.lang@h-da.de"
		}
	],
	"keywords" : [
		"TYPO3",
		"extension",
		"t3up",
		"container"
	],
	"type" : "typo3-cms-extension",
	"license" : "GPL-2.0-or-later",
	"require" : {
		"b13/container" : "^1.6.0 || ^2"
	},
    "autoload": {
        "psr-4": {
            "T3ac\\T3upContainer\\": "Classes"
        }
    },
	"replace" : {
		"typo3-ter/container_example" : "self.version"
	},
	"extra" : {
